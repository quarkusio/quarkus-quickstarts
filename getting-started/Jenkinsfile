pipeline {
    agent any
    stages {
        stage('Executar Pipeline Completa') {
            steps {
                echo 'Limpando o workspace para garantir um ambiente limpo...'
                cleanWs()
                echo 'Iniciando o script de automacao...'
                // Agora o Jenkins est√° na pasta correta e pode rodar o script diretamente
                sh './pipeline.sh'
            }
        }
    }
}